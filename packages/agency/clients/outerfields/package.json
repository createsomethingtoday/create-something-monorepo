{
  "name": "@create-something/outerfields",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=outerfields-pcn",
    "deploy:preview": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=outerfields-pcn --branch=preview",
    "migrate": "wrangler d1 migrations apply outerfields-db",
    "migrate:list": "wrangler d1 migrations list outerfields-db",
    "db:shell": "wrangler d1 execute outerfields-db --command",
    "types": "wrangler types"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@sveltejs/adapter-cloudflare": "^6.0.0",
    "@sveltejs/kit": "^2.15.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "svelte": "^5.16.0",
    "svelte-check": "^4.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "wrangler": "^4.59.0"
  },
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "lucide-svelte": "^0.562.0",
    "resend": "^6.7.0",
    "stripe": "^20.1.2"
  }
}
