{
	"name": "@create-something/agency",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=create-something-agency",
		"deploy:preview": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=create-something-agency --branch=preview",
		"cf-typegen": "wrangler types"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251128.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.10.1",
		"autoprefixer": "^10.4.22",
		"highlight.js": "^11.11.1",
		"marked": "^17.0.0",
		"mdsvex": "^0.12.6",
		"postcss": "^8.5.6",
		"postcss-import": "^16.1.1",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.9.3",
		"vite": "^7.1.10",
		"wrangler": "^4.59.0"
	},
	"dependencies": {
		"@create-something/canon": "workspace:*",
		"@create-something/tufte": "workspace:^",
		"lucide-svelte": "^0.562.0",
		"stripe": "^18.5.0"
	}
}
