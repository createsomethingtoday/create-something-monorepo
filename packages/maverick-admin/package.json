{
  "name": "@create-something/maverick-admin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "types": "wrangler types",
    "db:migrate": "wrangler d1 migrations apply maverick-db",
    "db:migrate:local": "wrangler d1 migrations apply maverick-db --local",
    "deploy": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=maverick-admin"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^4.7.2",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.5.0",
    "vite": "^6.0.0",
    "wrangler": "^4.59.0"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241022.0"
  }
}
