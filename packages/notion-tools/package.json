    "clean": "rm -rf pkg pkg-web target"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/create-something/create-something-monorepo.git",
    "directory": "packages/notion-tools"
  },
  "keywords": [
    "notion",
    "wasm",
    "webassembly",
    "rust"
  ],
