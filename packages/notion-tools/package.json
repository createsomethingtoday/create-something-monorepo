{
  "name": "@create-something/notion-tools",
  "version": "0.1.0",
  "description": "High-performance Notion data processing tools (WASM)",
  "type": "module",
  "main": "./pkg/notion_tools.js",
  "module": "./pkg/notion_tools.js",
  "types": "./pkg/notion_tools.d.ts",
  "exports": {
    ".": {
      "types": "./pkg/notion_tools.d.ts",
      "import": "./pkg/notion_tools.js"
    }
  },
  "files": [
    "pkg"
  ],
