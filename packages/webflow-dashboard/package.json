{
  "name": "@create-something/webflow-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=webflow-dashboard",
    "deploy:preview": "pnpm build && wrangler pages deploy .svelte-kit/cloudflare --project-name=webflow-dashboard --branch=preview"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251219.0",
    "@sveltejs/adapter-cloudflare": "^4.8.0",
    "@sveltejs/kit": "^2.8.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@create-something/canon": "workspace:^",
    "airtable": "^0.12.2",
    "lucide-svelte": "^0.454.0",
    "resend": "^6.7.0",
    "uuid": "^11.0.3"
  },
  "type": "module"
}
